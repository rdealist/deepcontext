{
  "name": "deepcontext",
  "version": "1.0.0",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm run dev:next\" \"npm run dev:electron\"",
    "dev:next": "cd app && npm run dev",
    "dev:electron": "bash scripts/dev-electron.sh",
    "build:electron": "tsc",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "cleanup": "pkill -f \"python.*engine/main.py\" || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "A privacy-first, local-only personal knowledge engine",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
